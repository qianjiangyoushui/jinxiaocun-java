<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Coreseed">

    <!-- 缓存条数 -->
    <cache size="2000" />

    <!-- 条件语句 -->
    <sql id="WhereSQL">
        <where>
            <if test="guid != null and guid != ''">
                AND T.GUID = #{guid}
            </if>
            <if test="code != null and code != ''">
                AND T.CODE = #{code}
            </if>
            <if test="name != null and name != ''">
                AND T.NAME = #{name}
            </if>
            <if test="variety != null and variety != ''">
                AND T.VARIETY = #{variety}
            </if>
            <if test="birthdate != null and birthdate != ''">
                AND T.BIRTHDATE = #{birthdate}
            </if>
            <if test="year != null and year != ''">
                AND T.YEAR = #{year}
            </if>
            <if test="selecttype != null and selecttype != ''">
                AND T.SELECTTYPE = #{selecttype}
            </if>
            <if test="combination != null and combination != ''">
                AND T.COMBINATION = #{combination}
            </if>
            <if test="feature != null and feature != ''">
                AND T.FEATURE = #{feature}
            </if>
            <if test="userid != null and userid != ''">
                AND T.USERID = #{userid}
            </if>
            <if test="companyid != null and companyid != ''">
                AND T.COMPANYID = #{companyid}
            </if>
            <if test="creatrdate != null and creatrdate != ''">
                AND T.CREATRDATE = #{creatrdate}
            </if>
            <if test="visible != null and visible != ''">
                AND T.VISIBLE = #{visible}
            </if>
            <if test="remark != null and remark != ''">
                AND T.REMARK = #{remark}
            </if>
            <if test="col1 != null and col1 != ''">
                AND T.COL1 = #{col1}
            </if>
            <if test="clo2 != null and clo2 != ''">
                AND T.CLO2 = #{clo2}
            </if>
        </where>
    </sql>

    <!-- 查找语句 -->
    <select id="find" resultType="Coreseed" parameterType="Coreseed" flushCache="false" useCache="true">
        SELECT
        T.GUID AS guid,
        T.CODE AS code,
        T.NAME AS name,
        T.VARIETY AS variety,
        T.BIRTHDATE AS birthdate,
        T.YEAR AS year,
        T.SELECTTYPE AS selecttype,
        T.COMBINATION AS combination,
        T.FEATURE AS feature,
        T.USERID AS userid,
        T.COMPANYID AS companyid,
        T.CREATRDATE AS creatrdate,
        T.VISIBLE AS visible,
        T.REMARK AS remark,
        T.COL1 AS col1,
        T.CLO2 AS clo2
        FROM coreseed T
        <include refid="WhereSQL" />
        ORDER BY SORT
    </select>

    <!-- 获取总记录数语句 -->
    <select id="findCount" resultType="java.lang.Integer" parameterType="Coreseed" flushCache="false" useCache="true">
        SELECT COUNT(*)  FROM coreseed T
        <include refid="WhereSQL" />
    </select>

    <!-- 插入语句 -->
    <insert id="insert" parameterType="Coreseed" flushCache="true">
        INSERT INTO coreseed
        (
        GUID,
        CODE,
        NAME,
        VARIETY,
        BIRTHDATE,
        YEAR,
        SELECTTYPE,
        COMBINATION,
        FEATURE,
        USERID,
        COMPANYID,
        CREATRDATE,
        VISIBLE,
        REMARK,
        COL1,
        CLO2
        )
        VALUES
        (
        #{guid,jdbcType=VARCHAR},
        #{code,jdbcType=VARCHAR},
        #{name,jdbcType=VARCHAR},
        #{variety,jdbcType=VARCHAR},
        #{birthdate,jdbcType=DATE},
        #{year,jdbcType=VARCHAR},
        #{selecttype,jdbcType=VARCHAR},
        #{combination,jdbcType=VARCHAR},
        #{feature,jdbcType=VARCHAR},
        #{userid,jdbcType=VARCHAR},
        #{companyid,jdbcType=VARCHAR},
        #{creatrdate,jdbcType=VARCHAR},
        #{visible,jdbcType=VARCHAR},
        #{remark,jdbcType=VARCHAR},
        #{col1,jdbcType=VARCHAR},
        #{clo2,jdbcType=VARCHAR}
        )
    </insert>

    <!-- 更新语句 -->
    <update id="update"  parameterType="Coreseed" flushCache="true">
        update coreseed
        <set>
            <if test="guid != null">GUID=#{guid},</if>
            <if test="code != null">CODE=#{code},</if>
            <if test="name != null">NAME=#{name},</if>
            <if test="variety != null">VARIETY=#{variety},</if>
            <if test="birthdate != null">BIRTHDATE=#{birthdate},</if>
            <if test="year != null">YEAR=#{year},</if>
            <if test="selecttype != null">SELECTTYPE=#{selecttype},</if>
            <if test="combination != null">COMBINATION=#{combination},</if>
            <if test="feature != null">FEATURE=#{feature},</if>
            <if test="userid != null">USERID=#{userid},</if>
            <if test="companyid != null">COMPANYID=#{companyid},</if>
            <if test="creatrdate != null">CREATRDATE=#{creatrdate},</if>
            <if test="visible != null">VISIBLE=#{visible},</if>
            <if test="remark != null">REMARK=#{remark},</if>
            <if test="col1 != null">COL1=#{col1},</if>
            <if test="clo2 != null">CLO2=#{clo2},</if>
        </set>
        WHERE GUID = #{guid}
    </update>

    <!-- 单选删除语句 -->
    <delete id="delete" parameterType="Coreseed" flushCache="true">
        DELETE FROM coreseed WHERE GUID = #{guid}
    </delete>

    <!-- 多选删除 -->
    <delete id="mulDel" parameterType="java.lang.String" flushCache="true">
        DELETE FROM coreseed WHERE GUID IN
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
